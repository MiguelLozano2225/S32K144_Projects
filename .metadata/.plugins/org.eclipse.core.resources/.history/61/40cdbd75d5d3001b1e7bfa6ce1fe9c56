#include "S32K144.h"
#include "pin_n_clock.h"
#include "My_led_n_sw.h"

int main(void)
{
	Init_clocks(PORT_C);
	Init_clocks(PORT_D);

	S32_NVIC->ICPR[PORTC_IRQn] |= 1<< (PORTC_IRQn %32);
	S32_NVIC->ISER[PORTC_IRQn] |= 1<< (PORTC_IRQn %32);
	S32_NVIC->IP[PORTC_IRQn] = 1;

	for(;;)
	{

	}

	return 0;
}

void PORTC_IRQHandler (void)
{
	int a;
	a++;
}
