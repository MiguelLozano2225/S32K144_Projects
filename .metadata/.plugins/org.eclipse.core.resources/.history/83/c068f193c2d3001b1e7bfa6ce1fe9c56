#ifndef MY_LED_N_SW_H_
#define MY_LED_N_SW_H_
#include "S32K144.h"


#define LED_BLUE_ON			PTD->PDOR &= ~1;
#define LED_BLUE_OFF		PTD->PDOR |=  1;

#define LED_RED	_ON
#define LED_RED_OFF

#define LED_GREEN_ON
#define LED_GREEN_OFF

#endif /* MY_LED_N_SW_H_ */
