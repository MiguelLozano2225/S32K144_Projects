#include "NVIC.h"

void EnableNVIC(uin8_t IRQn)
{
	S32_NVIC->ICPR[IRQn] = 1 << (IRQn % 32U);
	S32_NVIC->ISER[IRQn] = 1 << (IRQn % 32U);
}
void DisableNVIC(uin8_t IRQn)
{
	S32_NVIC->ICER[IRQn/4U] = 1 << (IRQn % 32U);
}
void SetPriority(uin8_t IRQn, uint8_t Prio)
{

}
