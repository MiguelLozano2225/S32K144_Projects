#include "NVIC.h"

void NVIC_EnableIRQ(uin8_t IRQn)
{
	S32_NVIC->ICPR[IRQn>>5U] = 1 << (IRQn & 0x1FU);
	S32_NVIC->ISER[IRQn] = 1 << (IRQn % 32U);
}

void NVIC_ClearPendingIRQ(uin8_t IRQn)
{

}

void NVIC_DisableIRQ(uin8_t IRQn)
{
	S32_NVIC->ICER[IRQn/4U] = 1 << (IRQn % 32U);
}
void NVIC_SetPriority(uin8_t IRQn, uint8_t Prio)
{

}
